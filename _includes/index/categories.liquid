<section>
    <h4 class="text-center">View wiki by repository</h4>
    <div class="row">
        {% for repo in site.data.repositories %}
            <a
                class="btn btn-outline-primary p-3
                col-12 col-sm-6 col-md-4 col-lg-3"
                href="repo.link">{{ repo.name }}</a>
        {% endfor %}
    </div>
</section>